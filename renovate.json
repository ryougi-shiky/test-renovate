{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
  ],
  "platformAutomerge": true,
  "customManagers": [
    {
      "customType": "regex",
      "description": "Maintain base AMI versions",
      "fileMatch": [
        "^base-ami\\.txt$"
      ],
      "matchStrings": [
        ".*amiFilter=(?<packageName>.*?)\n.*?(?<depName>[a-zA-Z0-9-_:]*)[ ]*?[:|=][ ]*?[\"|']?(?<currentValue>ami-[a-z0-9]{17})[\"|']?.*"
      ],
      "datasourceTemplate": "aws-machine-image",
      "versioningTemplate": "aws-machine-image"
    }
  ]
}