{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
  ],
  "platformAutomerge": true,
  "customManagers": [
    {
      "customType": "regex",
      "description": "Maintain base AMI versions",
      "fileMatch": [
        "base-ami.txt"
      ],
      "matchStrings": [
        "^(?<depName>aws:ami:[^:]+:[0-9]+:[^:]+)[ ]*[:=][ ]*(?<currentValue>ami-[a-z0-9]{17})"
      ],
      "datasourceTemplate": "aws-machine-image",
      "versioningTemplate": "aws-machine-image"
    }
  ]
}